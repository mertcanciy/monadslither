{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_CONTRACT_ADDRESS": "@react_app_contract_address",
    "REACT_APP_MONAD_TESTNET_CHAIN_ID": "@react_app_monad_testnet_chain_id",
    "REACT_APP_MONAD_TESTNET_RPC_URL": "@react_app_monad_testnet_rpc_url",
    "REACT_APP_MONAD_TESTNET_EXPLORER": "@react_app_monad_testnet_explorer"
  }
} 